{
  "name": "MindTOC",
  "description": "Editable page outliner with mind maps",
  "version": "1.0",
  "manifest_version": 3,
  "action": {},
  "content_scripts": [
    {
      "js": ["static/js/content.js"],
      "css": ["static/css/content.css"],
      "matches": ["http://*/*", "https://*/*"]
    }
  ],
  "permissions": ["scripting"],
  "icons": {
    "16": "logo16.png",
    "48": "logo48.png",
    "128": "logo128.png"
  }
}
