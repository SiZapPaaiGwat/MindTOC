{
  "name": "MindTOC",
  "description": "Editable page outliner with mind maps",
  "version": "1.0",
  "manifest_version": 3,
  "action": {},
  "content_scripts": [
    {
      "js": ["static/js/content.js"],
      "matches": ["http://*/*", "https://*/*"]
    }
  ],
  "permissions": ["activeTab", "scripting"],
  "icons": {
    "16": "logo192.png",
    "48": "logo192.png",
    "128": "logo192.png"
  }
}
